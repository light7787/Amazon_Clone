{
    "version": 2,
    "builds": [
      {
        "src": "build",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  